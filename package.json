{
  "name": "hexora-platform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "analyze": "ANALYZE=true npm run build",
    "export": "next build && next export",
    "optimize": "npm run clean && npm run build && npm run analyze",
    "docker:build": "docker build -t hexora-app:latest .",
    "docker:run": "docker run -d -p 3000:3000 --name hexora-container hexora-app:latest",
    "docker:stop": "docker stop hexora-container && docker rm hexora-container",
    "docker:logs": "docker logs hexora-container",
    "docker:shell": "docker exec -it hexora-container sh"
  },

